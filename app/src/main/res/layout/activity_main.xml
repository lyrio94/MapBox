<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ibFazer"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/ibQueFazer"
        android:layout_width="405dp"
        android:layout_height="114dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="@string/button"
        mapbox:layout_constraintBottom_toBottomOf="parent"
        mapbox:layout_constraintEnd_toEndOf="parent"
        mapbox:layout_constraintStart_toStartOf="parent"
        mapbox:srcCompat="@drawable/main_card"
        tools:ignore="RedundantDescriptionCheck" />

    <com.mapbox.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="411dp"
        android:layout_height="601dp"
        android:layout_marginBottom="2dp"
        android:contentDescription="@string/mapa"
        app:layout_constraintBottom_toTopOf="@+id/ibQueFazer"
        mapbox:layout_constraintEnd_toEndOf="parent"
        mapbox:layout_constraintStart_toStartOf="parent"
        mapbox:mapbox_cameraTargetLat="59.31"
        mapbox:mapbox_cameraTargetLng="18.06"
        mapbox:mapbox_cameraZoom="14.0" />
</androidx.constraintlayout.widget.ConstraintLayout>

